[sub_resource type="Shader" id="Shader_n45e2"]
code = "shader_type canvas_item;

uniform float density: hint_range(4.0, 25.0) = 10.0;
uniform vec4 color1: source_color;
uniform vec4 color2: source_color;

uniform float speed = 1.0;

void fragment() {
	float time_offset = TIME * speed;
	
	float x = (UV.x-.5);
	float y = (UV.y-.5);
	float v = (sqrt((x*x)+(y*y)) * density);
	float can_color = mod(v - time_offset, 2.0);
	vec4 color;
	if (can_color > 1.0) {
		color.rgb = color1.rgb;
	}else{
		color.rgb = color2.rgb;
	}
	color.a = 1.0;
	COLOR = vec4(color.rgba);
}

"

